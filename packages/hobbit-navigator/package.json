{
  "name": "mithril-hobbit-navigator",
  "version": "0.3.2",
  "description": "Component based routing using the HTML5 history API for Mithril",
  "repository": "Minivera/mithril-hobbit",
  "license": "MIT",
  "main": "./src/index.js",
  "scripts": {
    "build": "node ./build/build.js",
    "watch": "babel ./src -d . --ignore __tests__ --watch",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "clean": "git clean -fdX .",
    "lint": "eslint ./src",
    "test": "npm run test:jest && npm run test:bundlesize",
    "test:jest": "jest --env=jest-environment-jsdom-global",
    "test:coverage": "jest --env=jest-environment-jsdom-global --coverage",
    "test:bundlesize": "bundlesize"
  },
  "bundlesize": [
    {
      "path": "./umd/hobbit-navigator.min.js",
      "maxSize": "2.5 kB"
    }
  ],
  "module": "./es/index.js",
  "files": [
    "es",
    "umd",
    "index.js",
    "history.js",
    "router.js"
  ],
  "keywords": [
    "mithril",
    "routing",
    "history"
  ],
  "author": "Minivera",
  "peerDependencies": {
    "mithril": ">=1.1.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-3": "^6.22.0",
    "bundlesize": "^0.17.0",
    "eslint": "^4.19.1",
    "gzip-size": "^4.1.0",
    "jest": "^22.4.3",
    "jest-environment-jsdom": "^23.1.0",
    "jest-environment-jsdom-global": "^1.1.0",
    "mithril": "^1.1.6",
    "pretty-bytes": "^4.0.2",
    "raf": "^3.4.0",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "dependencies": {
    "path-to-regexp": "^2.2.1"
  }
}
